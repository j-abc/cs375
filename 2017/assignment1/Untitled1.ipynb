   "source": [
    "def get_boxcar(num_pts, total_length):\n",
    "    my_ones = np.ones(num_pts)/num_pts\n",
    "    my_zeros = np.zeros(total_length - num_pts)\n",
    "    return np.hstack([my_ones, my_zeros])"
   ]
